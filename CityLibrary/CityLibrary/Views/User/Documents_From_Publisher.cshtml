
@{
    ViewBag.Title = "Documents_From_Publisher";
}
@Html.ActionLink("Return to main Menu for user", "Main", "User")
<h2>Documents From Publisher</h2>

<table cellpadding="0" cellspacing="0" border="1">
    <tr>
        <th>Document ID</th>
        <th>Document Name </th>        
    </tr>
    @foreach (System.Data.DataRow row in Model.Rows)
    {
        <tr>
            <td>@row["DocId"]</td>
            <td>@row["Title"]</td>            
        </tr>
    }
</table>



@{
    ViewBag.Title = "Documents_From_Publisher";
}
<style>
    body {
        background: #333;
    }

    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    td, th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
        text-align: center;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }

    tr:nth-child(odd) {
        background-color: white;
    }
</style>
<h2>Documents From Publisher</h2>

<table cellpadding="0" cellspacing="0" border="1">
    <tr>
        <th>Document ID</th>
        <th>Document Name </th>        
    </tr>
    @foreach (System.Data.DataRow row in Model.Rows)
    {
        <tr>
            <td>@row["DocId"]</td>
            <td>@row["Title"]</td>            
        </tr>
    }
</table>


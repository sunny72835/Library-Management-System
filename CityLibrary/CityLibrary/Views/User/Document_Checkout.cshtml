
@{
    ViewBag.Title = "Document_Checkout";
}
@Html.ActionLink("Return to main Menu for user", "Main", "User")
<h2>Document Checkout Page</h2>
@{
    if (ViewBag.msg !=null)
    {
        <script type="text/javascript">
            alert('@ViewBag.msg');
        </script>
    }
}
@using (Html.BeginForm("DocumentCheckout", "User", FormMethod.Post))
{
    <table>
        <tr>
            <td>Enter Document Id: </td>
            <td>@Html.Editor("docId")</td>
        </tr>
        <tr>
            <td>Enter Copy No: </td>
            <td>@Html.Editor("copyNo")</td>
        </tr>
        <tr>
            <td>Enter Library Id: </td>
            <td>@Html.Editor("libId")</td>
        </tr>
        <tr>
            <td></td>
            <td><input id="btnChkOut" type="submit" value="Checkout" /></td>
        </tr>
    </table>
}

